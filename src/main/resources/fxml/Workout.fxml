<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="workout" maxHeight="800" maxWidth="1000" minHeight="400" minWidth="500" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.WorkoutController">
    <children>
        <Button fx:id="back" layoutX="14.0" layoutY="14.0" onMousePressed="#goBack" prefHeight="76.0" prefWidth="171.0" text="back" textFill="WHITE">
         <font>
            <Font name="Stencil" size="30.0" />
         </font></Button>
        <Button fx:id="customWorkout" layoutX="551.0" layoutY="326.0" mnemonicParsing="false" onMousePressed="#custom" prefHeight="108.0" prefWidth="403.0" text="Custom Workout" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
         <font>
            <Font name="Stencil" size="30.0" />
         </font>
        </Button>
        <Button fx:id="generatedWorkout" layoutX="40.0" layoutY="326.0" mnemonicParsing="false" onMousePressed="#generate" prefHeight="108.0" prefWidth="403.0" text="Generated Workout" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
         <font>
            <Font name="Stencil" size="30.0" />
         </font>
        </Button>
        <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="159.0" layoutY="97.0" text="Workout Planner" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Stencil" size="72.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
